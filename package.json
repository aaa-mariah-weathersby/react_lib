{
  "name": "react_lib_beta",
  "version": "1.0.0",
  "description": "library_config",
  "main": "index.js",
  "module": "build/library.module.js",
  "scripts": {
    "build": "rollup -c rollup.config.babel.js",
    "watch": "rollup -c rollup.config.babel.js -w",
    "examples:build": "cd \"./examples\" && \"./node_modules/.bin/webpack\"",
    "examples:watch": "cd \"./examples\" && \"./node_modules/.bin/webpack-dev-server\"",
    "start": "(npm run watch & npm run examples:watch)",
    "test": "jest --no-cache"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "useBuiltIns": "usage",
          "debug": false
        }
      ],
      "stage-0",
      "react",
      "jest"
    ],
    "plugins": [
      "lodash",
      "transform-decorators-legacy",
      "transform-class-properties"
    ]
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.jsx$": "babel-jest",
      "^.+\\.js$": "babel-jest"
    },
    "testURL": "http://localhost/",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "keywords": [
    "react",
    "component",
    "library",
    "popup",
    "menu"
  ],
  "author": "Mariah Weathersby",
  "license": "MIT",
  "dependencies": {
    "babel-preset-env": "^1.7.0",
    "clone": "^2.1.1",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "material-ui": "^1.0.0-beta.47",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-fontawesome": "^1.6.1",
    "webpack-dev-server": "^3.1.5"
  },
  "devDependencies": {
    "@material-ui/core": "^1.4.3",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.28.9",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "node-sass": "^4.7.2",
    "postcss-scss": "^1.0.3",
    "react-test-renderer": "^16.4.2",
    "rollup": "^0.56.2",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-postcss": "^1.2.8",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-watch": "^4.3.1",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2"
  }
}
